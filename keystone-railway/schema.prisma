datasource postgresql {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model Book {
  id   String  @id @default(cuid())
  name String?
  isbn String?
  slug String?
}

model User {
  id       String  @id @default(cuid())
  name     String?
  email    String? @unique
  password String?
  card     String? @unique
  pin      String?
}